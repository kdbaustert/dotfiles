#!/usr/bin/env sh

spacebar -m config position top
spacebar -m config height 30
spacebar -m config text_font "Gotham Books:Regular:16.0"
spacebar -m config icon_font "Hack Nerd Font:Regular:17.0"
spacebar -m config background_color 0xff202020
spacebar -m config foreground_color 0xffa8a8a8
spacebar -m config space_icon_color 0xff0099bd
spacebar -m config power_icon_color 0xff0099bd
spacebar -m config battery_icon_color 0xff0099bd
spacebar -m config dnd_icon_color 0xffa8a8a8
spacebar -m config clock_icon_color 0xff0099bd
spacebar -m config space_icon_strip      
spacebar -m config power_icon_strip  
spacebar -m config space_icon 
spacebar -m config clock_icon 
spacebar -m config dnd_icon 
spacebar -m config clock_format "%b %e %a %I:%M %p"

echo "spacebar configuration loaded.."

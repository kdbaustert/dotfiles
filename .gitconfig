helper = cache --timeout=36000
[filter "lfs"]
smudge = git-lfs smudge -- %f
process = git-lfs filter-process
required = true
clean = git-lfs clean -- %f
[init]
defaultBranch = master
[color]
ui = auto

[color "branch"]
current = yellow
local = 241
remote = green

[color "diff"]
meta = yellow
frag = magenta
old = red
new = green

[color "status"]
added = green
changed = yellow
untracked = magenta
